{
  "manifest_version": 3,
  "name": "CTRChart",
  "version": "0.1.2",
  "description": "Chart for CTR",
  "icons": {
    "512": "pie_512.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-3.6.4.min.js",
        "jquery.canvasjs.min.js",
        "content.js",
        "moment.js"
      ],
      "matches": [
        "https://apps-sogeti.capgemini.com/Applications/CTR/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "pie.png",
        "angles-left-solid.svg",
        "angles-right-solid.svg"
      ],
      "matches": [
        "https://apps-sogeti.capgemini.com/*"
      ]
    }
  ]
}